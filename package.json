{
  "name": "specialized-project",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "engines": {
    "node": "16.16.0"
  },
  "scripts": {
    "build": "webpack --mode production --openssl-legacy-provider",
    "spare_start": "npm run build && node src/server/index.js",
    "start": "react-scripts start --openssl-legacy-provider",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --openssl-legacy-provider",
    "server": "nodemon src/server/index.js --openssl-legacy-provider",
    "dev": "concurrently \"npm run server\" \"npm run client\" --openssl-legacy-provider"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "^5.0.1",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^4.15.1"
  }
}
